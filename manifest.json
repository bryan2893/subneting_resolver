{
    "manifest_version": 2,
    "name": "Subneting resolver",
    "description": "Chrome extension to resolve subneting given an ip addres and its net mask",
    "version": "1.0",
    "chrome_url_overrides": {
        "history": "app.html"
    },
    "browser_action": {
        "default_title": "Subneting resolver",
        "defaul_icon": ""
    },
    "background":{
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["cookies","tabs"]
}